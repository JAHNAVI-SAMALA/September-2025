class Solution:
    def isPalindrome(self, n: int) -> bool:
        if n < 0 or (n % 10 == 0 and n != 0):
            return False
        rev = 0
        while n > rev:
            pop = n % 10
            rev = rev*10 + pop
            n = n // 10
        return n == rev or n == rev//10
            
