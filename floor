def floor(arr,target):
    low = 0
    high = len(arr) -1
    ans = -1
    while(low <= high):
        mid = (low+high)//2
        if(arr[mid] <= target):# floor condition largest element <= x
            ans = mid
            low = mid + 1
        else:
            high = mid - 1
    return ans
arr = list(map(int,input().split()))
target = int(input())
print(floor(arr,target))
